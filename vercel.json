{
  "functions": {
    "api/predict.py": {
      "runtime": "vercel-python@3.11",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/predict", "dest": "/api/app.py" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
